; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Toonbox"
run/main_scene="res://core/main.tscn"
config/features=PackedStringArray("4.0", "C#")
run/max_fps=30
boot_splash/bg_color=Color(0.4, 0.4, 0.4, 1)
boot_splash/image="res://assets/splashscreen.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/icon.png"
config/windows_native_icon="res://assets/icon.ico"
boot_splash/minimum_display_time=10000

[autoload]

Build="*res://core/Build.cs"
ThemeManager="*res://core/ThemeManager.cs"
Global="*res://core/Global.cs"
ScaleManager="*res://core/ScaleManager.cs"

[debug]

file_logging/enable_file_logging.pc=false
multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/dpi/allow_hidpi=false
window/size/width=800

[dotnet]

project/assembly_name="Toonbox"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/panku_console/plugin.cfg", "res://addons/qodot/plugin.cfg", "res://addons/kanban_tasks/plugin.cfg", "res://addons/native_dialogs/plugin.cfg")

[gdnative]

singletons=[]

