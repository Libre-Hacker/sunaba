[application]

config/name="Sunaba"
run/main_scene="res://src/main.tscn"
boot_splash/image="res://sunaba-wordmark.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://sunaba-icon2.png"
config/windows_native_icon="res://sunaba-icon2.ico"

[autoload]

GameManager="*res://src/game_manager.gd"
Network="*res://src/netcode/network.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg", "res://addons/voxel-core/plugin.cfg" )

