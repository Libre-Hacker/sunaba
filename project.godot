[input_devices]

pointing/emulate_touch_from_mouse=true

[locale]

locale_filter=[1, ["en", "es", "ja"]]
translations=PackedStringArray()

[mono]

export/aot/enabled=true
export/aot/full_aot=true
export/aot/use_interpreter=false

[physics]

common/physics_jitter_fix=0.0
common/enable_pause_aware_picking=true
common/physics_fps=240

[rendering]

occlusion_culling/use_occlusion_culling=true
vram_compression/import_etc=true
misc/lossless_compression/force_png=true
gles3/shaders/shader_compilation_mode=1
